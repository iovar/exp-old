<ul class="list-group panel-body">
  {{#each library in model}}
  <li class="list-group-item">
    <h4>{{library.id}}</h4>
    <div><b>Resource</b>:
      <a {{bind-attr href=library.resource}}>{{library.resource}}</a>
    </div>
    <p>
      <b>Count</b>: {{library.count}},
      <b>github</b>: {{library.github}}
    </p>
  </li>
  {{/each}}
  {{page-turn hasPrevPage=hasPrevPage hasNextPage=hasNextPage }}
</ul>
