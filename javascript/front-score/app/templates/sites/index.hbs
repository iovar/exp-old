<ul class="list-group panel-body">
  {{#each site in model}}
  <li class="list-group-item">
    <h4>{{site.id}}</h4>
    <div><b>Resource</b>:
      <a {{bind-attr href=site.resource}}>{{site.resource}}</a>
    </div>
    <p>
      <b>Rank</b>: {{site.rank}}
    </p>
  </li>
  {{/each}}
  {{page-turn hasPrevPage=hasPrevPage hasNextPage=hasNextPage }}
</ul>
