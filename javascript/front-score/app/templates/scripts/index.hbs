<ul class="list-group panel-body">
  {{#each script in model}}
  <li class="list-group-item">
    <h4>{{script.id}}</h4>
    <div><b>Resource</b>:
      <a {{bind-attr href=script.resource}}>{{script.resource}}</a>
    </div>
    <p>
      <b>Count</b>: {{script.count}}
    </p>
  </li>
  {{/each}}
  {{page-turn hasPrevPage=hasPrevPage hasNextPage=hasNextPage }}
</ul>
